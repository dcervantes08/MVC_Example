@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    //@ViewBag.Title = "Gas Report Info";
}

<div class="row">
    <div class="col-lg-12">
        <div class="well">
            <div class="row">
                <div class="col-md-12" style="text-align: center">
                    <img src="~/Content/Images/tcot_logo.png" />
                </div>
            </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <h2 class="panel-title">Gas Production Details</h2>
                        </div>
                        <div class="panel-body">
                            <p>The Gas Production data displayed within the reports contained within this site
                                is data that is manipulated from the Texas RailRoad Commission. The process used to 
                                manipulate the data is a process created by T-COT Inc. and is unique only to T-COT Inc.
                                <br />
                            <br />
                                The following data is available within the Reports.
                                <h5>Years:</h5>
                                <ul class="list-unstyled">
                                    <li class="list-group-item">2010: Jan - Dec</li>
                                    <li class="list-group-item">2011: Jan - Dec</li>
                                    <li class="list-group-item">2012: Jan - Dec</li>
                                    <li class="list-group-item">2013: Jan - Dec</li>
                                    <li class="list-group-item">2014: Jan - Dec</li>
                                    <li class="list-group-item">2015: Jan - Dec</li>
                                    <li class="list-group-item">2016: Jan - Dec</li>
                                </ul>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="panel panel-info">
                        <div class="panel-body">
                            <img class="img-responsive" src="~/Content/Images/money-gas.jpg" style="width: 100%"/>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
