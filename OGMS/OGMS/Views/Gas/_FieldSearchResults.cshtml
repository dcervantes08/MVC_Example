@model List<OGMS.Models.GasModels.GasProdPerField>

<div class="well">
    <h4>Gas Production Per Field Search Results</h4>

    <table class="table table-bordered table-responsive table-striped">
        <thead>
            <tr>
                <th>
                    Operator Id
                </th>
                <th>
                    Operator Name
                </th>
                <th>
                    Field Id
                </th>
                <th>
                    Field Name
                </th>
                <th>
                    Production Average
                </th>
                <th>
                    Total Prod
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var gasProdResults in Model)
            {
                <tr>
                    <td>
                        @gasProdResults.OperatorId
                    </td>
                    <td>
                        @gasProdResults.OperatorName
                    </td>
                    <td>
                        @gasProdResults.FieldNumber
                    </td>
                    <td>
                        @gasProdResults.FieldName
                    </td>
                    <td>
                        @gasProdResults.ProdAvg
                    </td>
                    <td>
                        @gasProdResults.TotalProd
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

